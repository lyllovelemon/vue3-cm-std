{
  "name": "vue3-cm-std",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "scripty",
    "build:images": "scripty",
    "build:styles": "scripty",
    "build:scripts": "scripty",
    "build:hash": "scripty",
    "build:dev": "vue-tsc --noEmit && vite build --mode=development",
    "serve": "vite preview",
    "preview": "vite preview",
    "precommit": "lint-staged",
    "prepush": "npm run test",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint *.js",
    "test": "cross-env NODE_ENV=test mocha tests/",
    "client": "npm-run-all --parallel client:*",
    "client:reload-server": "livereload client/",
    "client:static-server": "http-server client/",
    "prebuild": "rimraf dist && make-dir dist/{images,styles,scripts}"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.less": "stylelint",
    "*.css": "stylelint",
    "*.json": "jsonlint --quite",
    "*.md": "markdownlint --config .markdownlint.json"
  },
  "dependencies": {
    "@vueuse/core": "^8.2.0",
    "axios": "^0.26.1",
    "echarts": "^5.3.1",
    "element-plus": "^2.1.7",
    "vue": "^3.2.25",
    "vue-router": "^4.0.13",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.0",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "chalk": "^2.4.2",
    "child_process": "^1.0.2",
    "cpr": "^3.0.1",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "cssmin": "^0.4.3",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^8.5.0",
    "hashmark": "^5.0.0",
    "http-server": "^14.1.0",
    "husky": "^7.0.4",
    "imagemin-cli": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^12.3.7",
    "livereload": "^0.9.3",
    "make-dir-cli": "^3.0.0",
    "mocha": "^9.2.2",
    "npm-run-all": "^4.1.5",
    "nprogress": "^0.2.0",
    "onchange": "^7.1.0",
    "replaceinfiles": "^1.1.4",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.49.10",
    "scripty": "^2.1.0",
    "shelljs": "^0.8.5",
    "uglify-es": "^3.3.9",
    "vite": "^2.8.0",
    "vite-plugin-mock": "^2.9.6"
  }
}
