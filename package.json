{
  "name": "vue3-cm-std",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vue-tsc --noEmit && vite build --mode=development",
    "serve": "vite preview",
    "preview": "vite preview",
    "lint": "eslint --fix --ext .js,.vue src/",
    "test": "cross-env NODE_ENV=test jest tests/",
    "bash-script": "echo Hello $npm_package_name",
    "win-script": "echo Hello %npm_package_name%",
    "cover": "nyc --reporter=html npm test",
    "cover:cleanup": "rimraf coverage && rimraf .nyc_output",
    "cover:archive": "cross-var \"make-dir coverage_archive/$npm_package_version && cpr coverage/* coverage_archive/$npm_package_version -o\"",
    "cover:serve": "cross-var http-server coverage_archive/$npm_package_version -p $npm_package_config_port",
    "cover:open": "cross-var opn http://localhost:$npm_package_config_port",
    "precover": "npm run cover:cleanup",
    "postcover": "npm-run-all cover:archive cover:cleanup --parallel cover:serve cover:open"
  },
  "dependencies": {
    "@vueuse/core": "^8.2.0",
    "axios": "^0.26.1",
    "echarts": "^5.3.1",
    "element-plus": "^2.1.7",
    "vue": "^3.2.25",
    "vue-router": "^4.0.13",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.0",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "cpr": "^3.0.1",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jsonwebtoken": "^8.5.1",
    "make-dir-cli": "^3.0.0",
    "nprogress": "^0.2.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.49.10",
    "vite": "^2.8.0",
    "vite-plugin-mock": "^2.9.6"
  }
}
